<launch>
    <arg name="device_ip1" default="192.168.1.200"/>
    <arg name="msop_port1" default="2368"/>
    <arg name="difop_port1" default="2369"/>
    <arg name="device_ip2" default="192.168.1.201"/>
    <arg name="msop_port2" default="2370"/>
    <arg name="difop_port2" default="2371"/>
    <arg name="use_time_service" default="false"/>

    <node pkg="lslidar_driver" type="lslidar_driver_node" name="lslidar_driver_node"
          output="screen" ns="LeftLidar">
        <!--  <param name="pcap" value="$(find lslidar_driver)/pcap/xxx.pcap"/>-->
        <param name="lidar_ip" value="$(arg device_ip1)"/>
        <param name="msop_port" value="$(arg msop_port1)"/>
        <param name="difop_port" value="$(arg difop_port1)"/>
        <param name="add_multicast" value="false"/>
        <param name="group_ip" value="224.1.1.2"/>
        <param name="use_time_service" value="$(arg use_time_service)"/>
        <param name="frame_id" value="laser_link"/>
        <param name="is_add_frame_" value="false"/>
        <param name="min_range" value="0.15"/>
        <param name="max_range" value="500.0"/>
        <param name="pointcloud_topic" value="lslidar_point_cloud"/>
        <param name="Vertical_angle_compensation" value="-28.0"/>   <!--垂直角度补偿-->
        <param name="scan_start_angle" value="-60"/>
        <param name="scan_end_angle" value="60"/>
    </node>

    <node pkg="lslidar_driver" type="lslidar_driver_node" name="lslidar_driver_node"
          output="screen" ns="RightLidar">
        <!--    <param name="pcap" value="$(find lslidar_driver)/pcap/xxx.pcap"/>-->
        <param name="lidar_ip" value="$(arg device_ip2)"/>
        <param name="msop_port" value="$(arg msop_port2)"/>
        <param name="difop_port" value="$(arg difop_port2)"/>
        <param name="add_multicast" value="false"/>
        <param name="group_ip" value="224.1.1.2"/>
        <param name="use_time_service" value="$(arg use_time_service)"/>
        <param name="frame_id" value="laser_link"/>
        <param name="is_add_frame_" value="false"/>
        <param name="min_range" value="0.15"/>
        <param name="max_range" value="500.0"/>
        <param name="pointcloud_topic" value="lslidar_point_cloud"/>
        <param name="Vertical_angle_compensation" value="-28.0"/>   <!--垂直角度补偿-->
        <param name="scan_start_angle" value="-60"/>
        <param name="scan_end_angle" value="60"/>
    </node>

<!--    <node pkg="tf" type="static_transform_publisher" name="base_link2laser"-->
<!--          args="0 0 0 0 0 0 world laser_link_right 100"/>-->
<!--    //-->
<!--    <node pkg="tf" type="static_transform_publisher" name="base_link2laser2"-->
<!--          args="0 0 2 0 0 0 world laser_link_left 100"/>-->
</launch>

